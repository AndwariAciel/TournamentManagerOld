<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="hbox" alignment="CENTER" maxWidth="500.0" minHeight="30.0" prefHeight="30.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="status" alignment="CENTER" prefWidth="60.0" text="Playing" textFill="GREY">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
      <Label fx:id="player1" alignment="CENTER" prefWidth="190.0" text="Player1">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="scoreP1" alignment="CENTER" contentDisplay="CENTER" prefWidth="25.0" text="-" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label alignment="CENTER" prefWidth="10.0" text=":">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="scoreP2" alignment="CENTER" prefWidth="25.0" text="-">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="player2" alignment="CENTER" prefWidth="190.0" text="Player2">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</HBox>
