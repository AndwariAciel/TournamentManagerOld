<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="700.0" prefWidth="1280.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.andwari.main.MainWindowController">
	<center>
		<ImageView pickOnBounds="true" preserveRatio="true"
			BorderPane.alignment="CENTER">
			<image>
				<Image url="@../images/banner.jpg" />
			</image>
		</ImageView>
	</center>
	<bottom>
		<FlowPane alignment="CENTER" columnHalignment="CENTER"
			prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
			<children>
				<Label text="Andwari's Tournament Manager">
					<font>
						<Font name="Cambria Math" size="48.0" />
					</font>

				</Label>
				<Label fx:id="versionLabel" text="version">
					<padding>
						<Insets left="20.0" />
					</padding>
					<font>
						<Font size="20.0" />
					</font>
				</Label>
			</children>
		</FlowPane>
	</bottom>
</BorderPane>
